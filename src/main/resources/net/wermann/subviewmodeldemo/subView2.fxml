<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="net.wermann.subviewmodeldemo.controller.SubViewController2" alignment="CENTER" spacing="10">
    <ListView fx:id="personListView2" prefWidth="200"/>
    <HBox alignment="CENTER" spacing="10">
        <TextField fx:id="personNameField2" promptText="Enter Name"/>
        <Button text="Add" onAction="#addPerson"/>
        <Button text="Delete" onAction="#deletePerson"/>
    </HBox>
</VBox>
